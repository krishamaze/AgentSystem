# Quick project resume - shows status and suggests next action
param([string]$ProjectName)

if (-not $ProjectName) {
    Write-Output "Available projects:"
    Get-ChildItem ".\Projects" -Directory | ForEach-Object {
        Write-Output "  - $($_.Name)"
    }
