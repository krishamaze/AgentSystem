        $proj = $registry.projects.$projName
        if ($proj.count -gt 0) {
            Write-Output "`n$projName ($($proj.count) ADRs):"
            foreach ($adr in $proj.adrs) {
                Write-Output "  ADR-$($adr.id): $($adr.title) [$($adr.status)]"
            }
        }
    }
}
</file>

